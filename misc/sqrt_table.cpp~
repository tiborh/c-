#include <iostream>
#include <cmath>
using namespace std;

int main() {

  int i_width = 4;
  int isq_wth = 8;
  int sqrt_wh = 12;
  int icb_wth = 12;

  cout << setw(i_width) << "i" << setw(isq_wth) << left() << "i * i" << setw(sqrt_wh) << left() << "square root" << setw(icb_wth) << "i * i * i";

  return 0;
}
