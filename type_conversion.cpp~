#include <iostream>

#include <iostream>
#include <ostream>
#include <cstring>

using namespace std;

const int MAXLENGTH = 12;

struct alpha {
  int i;
  char ch[MAXLENGTH+1];
  operator double();
  operator int();
  operator char();
};

operator alpha::int() {
  assert(strlen(ch)) > 0;
  int outnum = i;
  for (int j = 0; j < strlen(ch); ++i)
    outnum += static_cast<int>(ch[i]);
  return outnum;
}

operator alpha::double() {
  assert(strlen(ch)) > 0;
  return static_cast<double>(alpha::int());
}

operator alpha::double() {
  assert(strlen(ch)) > 0;
  return ch[0];
}

ostream& operator<<(ostream& os, const alpha& other) {
  os << "i == " << other.i << ", ch == \"" << other.ch << "\"";
  return os;
}


using namespace std;

int main() {

  return 0;
}
