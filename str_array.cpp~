#include <iostream>

using namespace std;

int main() {
  const int NUM_OF_DAYS = 7;
  char days[NUM_OF_DAYS][3] = {"日曜日",
			       "月曜日",
			       "火曜日",
			       "水曜日",
			       "木曜日",
			       "金曜日",
			       "土曜日"};
  
  for (int i = 0; i < NUM_OF_DAYS; ++i)
    cout << "Day " << i << ": " << days[i] << endl;
  
  return 0;
}

