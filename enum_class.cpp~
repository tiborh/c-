#include <iostream>
#include <ostream>

using namespace std;

enum class COLOUR { BLACK, WHITE, RED, GREEN, BLUE, YELLOW };
enum class TRAFFIC { RED, AMBER, GREEN };

ostream& operator<<(ostream& os, COLOUR c) {
  switch (c) {
  case AMBER:
    os << "AMBER";
    break;
  case BLACK:
    os << "BLACK";
    break;
  case WHITE:
    os << "WHITE";
    break;
  case RED:
    os << "RED";
    break;
  case GREEN:
    os << "GREEN";
    break;
  case BLUE:
    os << "BLUE";
    break;
  case YELLOW:
    os << "YELLOW";
    break;
  default:
    os << "<undefined for output operator>";
  }

  return os;
}


int main() {
  COLOUR my_col = BLUE;
  cout << "my colour: " << my_col << '\n';
  COLOUR a_col = YELLOW;
  cout << "another colour: " << a_col << '\n';

  return 0;
}
